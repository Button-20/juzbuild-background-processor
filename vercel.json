{
  "version": 2,
  "public": false,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "dist/api.js": {
      "maxDuration": 900,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/api.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
