{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 900,
        "memory": 1024
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "env": {
    "NODE_ENV": "production"
  }
}
